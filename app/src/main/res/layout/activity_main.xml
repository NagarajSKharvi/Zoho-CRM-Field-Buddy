<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="128">

        <ImageButton
            android:id="@+id/imageButton27"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="51dp"
            android:layout_marginStart="51dp"
            android:layout_marginTop="96dp"
            android:background="@android:color/transparent"
            app:srcCompat="@mipmap/ic_schedule" />

        <ImageButton
            android:id="@+id/imageButton28"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/imageButton27"
            android:layout_marginEnd="41dp"
            android:layout_marginRight="41dp"
            android:background="@android:color/transparent"
            app:srcCompat="@mipmap/ic_completed" />

        <ImageButton
            android:id="@+id/imageButton29"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_alignLeft="@+id/imageButton27"
            android:layout_alignStart="@+id/imageButton27"
            android:layout_below="@+id/imageButton27"
            android:layout_marginTop="20dp"
            android:background="@android:color/transparent"
            app:srcCompat="@mipmap/ic_contacts" />

        <ImageButton
            android:id="@+id/imageButton32"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_alignBottom="@+id/imageButton29"
            android:layout_alignEnd="@+id/imageButton28"
            android:layout_alignRight="@+id/imageButton28"
            android:layout_alignTop="@+id/imageButton29"
            android:background="@android:color/transparent"
            app:srcCompat="@mipmap/ic_surveys" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageButton27"
            android:layout_alignEnd="@+id/imageButton27"
            android:layout_alignRight="@+id/imageButton27"
            android:layout_marginBottom="12dp"
            android:layout_marginEnd="26dp"
            android:layout_marginRight="26dp"
            android:text="Scheduled"
            android:textAlignment="center"
            android:textColor="?attr/colorAccent"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView"
            android:layout_alignEnd="@+id/imageButton28"
            android:layout_alignRight="@+id/imageButton28"
            android:layout_marginEnd="23dp"
            android:layout_marginRight="23dp"
            android:text="Completed"
            android:textColor="@color/colorAccent"
            android:textColorLink="@color/colorAccent"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageButton29"
            android:layout_alignEnd="@+id/textView"
            android:layout_alignRight="@+id/textView"
            android:layout_marginBottom="13dp"
            android:text="Customers"
            android:textColor="@color/colorAccent"
            android:textColorLink="@color/colorAccent"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView3"
            android:layout_alignEnd="@+id/textView2"
            android:layout_alignLeft="@+id/textView2"
            android:layout_alignRight="@+id/textView2"
            android:layout_alignStart="@+id/textView2"
            android:layout_alignTop="@+id/textView3"
            android:text="Job Cards"
            android:textColor="@color/colorAccent"
            android:textSize="14sp" />

    </RelativeLayout>
</LinearLayout>